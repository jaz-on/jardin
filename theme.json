{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"slug": "background",
					"color": "#282a2e",
					"name": "Background"
				},
				{
					"slug": "foreground",
					"color": "#ffffff",
					"name": "Foreground"
				},
				{
					"slug": "accent",
					"color": "#ff6b6b",
					"name": "Accent"
				},
				{
					"slug": "accent-hover",
					"color": "#ff8e8e",
					"name": "Accent Hover"
				},
				{
					"slug": "logo",
					"color": "#fcae11",
					"name": "Logo"
				},
				{
					"slug": "muted",
					"color": "#b3b3b3",
					"name": "Muted"
				},
				{
					"slug": "surface",
					"color": "#3a3a3a",
					"name": "Surface"
				}
			],
			"custom": {
				"theme-variants": {
					"variant-light": {
						"background": "#ffffff",
						"foreground": "#1a1a1a",
						"accent": "#2563eb",
						"logo": "#fcae11",
						"muted": "#6b7280",
						"surface": "#f9fafb"
					},
					"variant-dark-blue": {
						"background": "#0a0a19",
						"foreground": "#f9f0ff",
						"accent": "#c7abe3",
						"logo": "#fcae11",
						"muted": "#b8a8d9",
						"surface": "#1a1a2e"
					}
				}
			}
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			],
			"blockGap": true,
			"margin": true,
			"padding": true
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "\"Inter\",-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,\"Helvetica Neue\",Arial,sans-serif",
					"slug": "body",
					"name": "Body Text",
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/inter/InterVariable.woff2"
							],
							"fontDisplay": "swap"
						},
						{
							"fontFamily": "Inter",
							"fontStyle": "italic",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/inter/InterVariable-Italic.woff2"
							],
							"fontDisplay": "swap"
						}
					]
				},
				{
					"fontFamily": "\"Inter\",-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,\"Helvetica Neue\",Arial,sans-serif",
					"slug": "heading",
					"name": "Headings"
				},
				{
					"fontFamily": "\"Calligraffitti\",cursive",
					"slug": "calligraffitti",
					"name": "Calligraffitti",
					"fontFace": [
						{
							"fontFamily": "Calligraffitti",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/calligraffitti/Calligraffitti-Regular.woff2",
								"file:./assets/fonts/calligraffitti/Calligraffitti-Regular.ttf"
							],
							"fontDisplay": "swap"
						}
					]
				},
				{
					"fontFamily": "\"Fira Code\",\"SF Mono\",\"Monaco\",\"Inconsolata\",\"Roboto Mono\",\"Courier New\",monospace",
					"slug": "code",
					"name": "Code",
					"fontFace": [
						{
							"fontFamily": "Fira Code",
							"fontStyle": "normal",
							"fontWeight": "300 700",
							"src": [
								"file:./assets/fonts/fira-code/FiraCode-VF.woff2"
							],
							"fontDisplay": "swap"
						}
					]
				},
				{
					"fontFamily": "\"Atkinson Hyperlegible\",-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,\"Helvetica Neue\",Arial,sans-serif",
					"slug": "atkinson",
					"name": "Atkinson Hyperlegible",
					"fontFace": [
						{
							"fontFamily": "Atkinson Hyperlegible",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/atkinson-hyperlegible/AtkinsonHyperlegible-Regular.woff2"
							],
							"fontDisplay": "swap"
						},
						{
							"fontFamily": "Atkinson Hyperlegible",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/atkinson-hyperlegible/AtkinsonHyperlegible-Bold.woff2"
							],
							"fontDisplay": "swap"
						},
						{
							"fontFamily": "Atkinson Hyperlegible",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/atkinson-hyperlegible/AtkinsonHyperlegible-Italic.woff2"
							],
							"fontDisplay": "swap"
						},
						{
							"fontFamily": "Atkinson Hyperlegible",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/atkinson-hyperlegible/AtkinsonHyperlegible-BoldItalic.woff2"
							],
							"fontDisplay": "swap"
						}
					]
				}
			],
			"fontSizes": [
				{
					"slug": "xs",
					"size": "0.75rem",
					"name": "Extra Small"
				},
				{
					"slug": "base",
					"size": "1.125rem",
					"name": "Base"
				},
				{
					"slug": "lg",
					"size": "1.25rem",
					"name": "Large"
				},
				{
					"slug": "xl",
					"size": "1.5rem",
					"name": "Extra Large"
				},
				{
					"slug": "2xl",
					"size": "1.875rem",
					"name": "2XL"
				},
				{
					"slug": "h1-hero",
					"size": "clamp(4rem, 15vw, 13rem)",
					"name": "H1 Hero"
				}
			],
			"lineHeight": {
				"tight": "1.2",
				"normal": "1.5",
				"relaxed": "1.6"
			},
			"dropCap": false
		},
		"blockStyles": [
			{
				"name": "butter-effect",
				"label": "Butter Effect",
				"blocks": ["core/site-title"],
				"isDefault": true
			}
		],
		"blocks": {
			"core/heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|heading",
					"fontWeight": "600",
					"lineHeight": "1.4"
				},
				"color": {
					"text": "var:preset|color|foreground"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "0"
					}
				}
			},
			"core/paragraph": {
				"typography": {
					"fontFamily": "var:preset|font-family|body",
					"fontSize": "var:preset|font-size|base",
					"lineHeight": "var:preset|typography|line-height|relaxed"
				},
				"color": {
					"text": "var:preset|color|foreground"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var:preset|spacing|4"
					}
				}
			},
			"core/link": {
				"color": {
					"text": "var:preset|color|accent"
				},
				"typography": {
					"textDecoration": "underline",
					"textDecorationSkipInk": "none"
				},
				":hover": {
					"color": {
						"text": "var:preset|color|accent-hover"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"outline": {
						"offset": "3px",
						"width": "3px",
						"style": "solid",
						"color": "var:preset|color|accent"
					}
				}
			},
			"core/button": {
				"color": {
					"background": "var:preset|color|accent",
					"text": "var:preset|color|background"
				},
				"typography": {
					"fontWeight": "500"
				},
				"spacing": {
					"padding": {
						"top": "0.75rem",
						"right": "1.5rem",
						"bottom": "0.75rem",
						"left": "1.5rem"
					}
				},
				":hover": {
					"color": {
						"background": "var:preset|color|accent-hover"
					}
				},
				":focus": {
					"border": {
						"color": "var:preset|color|accent",
						"width": "2px",
						"style": "solid"
					},
					"outline": {
						"offset": "0",
						"width": "0",
						"style": "none"
					}
				}
			},
			"core/code": {
				"typography": {
					"fontFamily": "var:preset|font-family|code"
				}
			},
			"core/preformatted": {
				"typography": {
					"fontFamily": "var:preset|font-family|code"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": true,
					"fontSize": true,
					"fontWeight": true,
					"lineHeight": true,
					"textAlign": true
				},
				"color": {
					"text": true
				},
				"spacing": {
					"margin": true,
					"padding": true
				}
			},
			"jardin/table-of-contents": {
				"spacing": {
					"margin": true,
					"padding": true
				},
				"color": {
					"background": true,
					"text": true
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var:preset|color|background",
			"text": "var:preset|color|foreground"
		},
		"typography": {
			"fontFamily": "var:preset|font-family|body",
			"fontSize": "var:preset|font-size|base",
			"lineHeight": "var:preset|typography|line-height|relaxed"
		},
		"spacing": {
			"padding": {
				"top": "0",
				"right": "0",
				"bottom": "0",
				"left": "0"
			}
		},
		"elements": {
			"link": {
				"color": {
					"text": "var:preset|color|accent"
				},
				":hover": {
					"color": {
						"text": "var:preset|color|accent-hover"
					}
				},
				":focus": {
					"outline": {
						"offset": "3px",
						"width": "3px",
						"style": "solid",
						"color": "var:preset|color|accent"
					}
				}
			}
		},
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontFamily": "var:preset|font-family|calligraffitti",
					"fontSize": "var:preset|font-size|h1-hero",
					"fontWeight": "700",
					"lineHeight": "1.2",
					"textAlign": "center"
				},
				"color": {
					"text": "var:preset|color|foreground"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var:preset|color|foreground"
						},
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"color": {
								"text": "var:preset|color|foreground"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"color": {
								"text": "var:preset|color|foreground"
							},
							"typography": {
								"textDecoration": "none"
							}
						}
					}
				}
			},
			"jardin/table-of-contents": {
				"spacing": {
					"padding": {
						"top": "var:preset|spacing|4",
						"right": "var:preset|spacing|4",
						"bottom": "var:preset|spacing|4",
						"left": "var:preset|spacing|4"
					},
					"margin": {
						"top": "var:preset|spacing|6",
						"bottom": "var:preset|spacing|6"
					}
				},
				"elements": {
					"link": {
						"color": {
							"text": "var:preset|color|accent"
						},
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"color": {
								"text": "var:preset|color|accent-hover"
							}
						}
					}
				}
			}
		}
	}
}
